Algoritmo "preço"


Var
    p :caractere
    v:real
    q :real

Inicio
    escreval("nome do produto")
      leia (p)
    escreval("qual o valor unitario?")
      leia (v)
    escreval("qual a quantidade?")
      leia (q)
     v <- (q * v)
      
    escreval("o total a pagar por ",q," unidades do produto ",p," é de r$ ", v )
      


Fimalgoritmo