












Algoritmo "semnome"

Var
   d :real
   v :real
    t : real

Inicio
      escreval(" qual a distância percorrida")
      leia (d)
      escreval(" qual a velocidade")
      leia (v)
       se d / v >= 1 entao
       t <- d / v
       senao
       t <- d / v
       fimse
      escreval("o tempo necessario para percorrer", d , " é ", t, " hora(s)")


Fimalgoritmo